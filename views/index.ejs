  <section class="playing small medium large">
    <div class="music"></div>
    <div class="playlist ">
      <div class="current"></div>
      <ul class="list"></ul>
    </div>
  </section>







  <section id="playList" class="container"> </section>

  <section id="albums" class="container"> </section>

  <!-- Templates -->
  <script id="player-template" type="text/x-handlebars-template">
    <figure class="image relative">
        <img src="img/songs/{{album_cover}}" width="320" height="320" alt="{{name}} - {{author}}">
        <div class="actions padding right-text">
          <a href="#" class="action icon-add"></a>
          <a href="#" class="action icon-love"></a>
          <a href="#" class="action icon-share"></a>
        </div>
      </figure>
      <div class="padding">
        <div class="progress">
          <span class="min">0:45</span>
          <div class="total">
            <div class="state">
              <div class="circle">
                
              </div>
            </div>
          </div>
          <span class="max">{{length}}</span>
        </div>
        <div class="description center-text border-bottom">
          <p class="name">
            {{name}}
          </p>
          <p class="author">
            {{author}}
          </p>
        </div>
        <audio id="audioPlay">
          <source src="media/{{src}}" type="audio/mpeg">
          your browser does not support this audio format
        </audio>
        <div class="play center-text padding-tb">
          <a href="" class="action gray icon-prev"></a>
          <a href="" class="action gray icon-play playSound"></a>
          <a href="" class="action gray icon-stop pauseSound"></a>
          <a href="" class="action gray icon-next"></a>
          <a href="" class="action gray icon-random"></a>
          <a href="" class="action gray icon-vol"></a>
        </div>
      </div>
  </script>

  <script id="current-album-template" type="text/x-handlebars-template">
    <p class="name">
      {{name}}
    </p>
    <p class="author">
      {{author}}
    </p>
  </script>

  <script id="song-template" type="text/x-handlebars-template">
    <span class="selecMusic">
      {{name}} - ({{length}})
    </span>
    <span class="right">
      <a href="" class="action icon-add gray"></a>
      <a href="" class="action icon-love gray"></a>
      <a href="" class="action icon-share gray"></a>
    </span>
  </script>



  <script id="playList-template" type="text/x-handlebars-template">
    <figure class="image">
        <img src="/img/songs/{{img}}" alt="{{name}}">
    </figure>
    <div class="infoPlayList">
      <p class="name">
        {{name}}
      </p>
      <p class="">
        {{by}}
      </p>
    </div>
  </script>

  <script id="album-template" type="text/x-handlebars-template">
    <figure class="image">
      <img src="img/songs/{{cover}}" width="128" height="128" alt="{{name}} - {{author}}">
    </figure>
    <p class="name">
      {{name}}
    </p>
    <p class="author">
      {{author}}
    </p>
  </script>
  <script type="text/javascript" src="js/app.js"></script>

